!* Outfix version 1.0 Gero Lipsmeier/UniBi 1.04.99prog def outfixversion 5.0set more offlocal args = 1local vars = 1local col_0 = 0while "`1'" != ""{	if `args'/2 ~= int(`args'/2) {		confirm integer number `1'		local col_`vars' = `1'			local lag = `vars' - 1		if `col_`lag'' >= `1' {			di in red "Column for `2'  to small			exit 9}		}	if `args'/2 == int(`args'/2) {		confirm var `1'		local var_`vars' = "`1'"		local vars = `vars' +1		}			mac shift	local args = `args'+ 1	}local i = 1while `i' <= _N {	local k = 1		local dic= ""		while `k' < `vars' {		local dicmd = "_col(`col_`k'') `var_`k''[`i']"			local dic= "`dic' `dicmd'"		local k = `k'+1		}	di `dic'	local i = `i' + 1	}set more onend