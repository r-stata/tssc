f mict_model_gap.sthlp
f mict_model_initial.ado
f mict_model_initial.sthlp
f mict_model_terminal.ado
f mict_model_terminal.sthlp
f mict_prep.ado
f mict_prep.sthlp
f mict_example1.do
f mict_example2.do
f mvad.dta
f mvadmar.dta
