/*  archlm2  VERSION 1.0.0  21dec2002*/VERSION 8.0INCLUDE _std_largeINCLUDE headerHELP hlp1, view("help archlm")RESET res1DIALOG main, label("archlm2 - Perform an LM test for ARCH on panel data") /*		*/ tabtitle("Main")BEGIN  /*--+----1----+----2----+----3----+----4----+----5----+----6----+----7----+*/   DEFINE _x _xsetbu  DEFINE _y _top  INCLUDE _bu_tsset  CHECKBOX ck_lags         _lft      _ms        _iwd     .,		/*  		*/label("Specify the lag orders to be tested")		/*  		*/clickon(gaction main.ed_lags.enable)			/*		*/clickoff(gaction main.ed_lags.disable)  EDIT     ed_lags         _indent2  _ss        _ibwd    .,		/*  		*/label("Specify the lag orders to be tested")		/*  		*/option(lags)  CHECKBOX ck_force        _lft      _ls        _iwd     .,		/*  		*/label("Allow test after -regress, robust- ")/*  		*/option(force)ENDPROGRAM commandBEGIN	put "archlm2 "	beginoptions		if main.ck_lags {			require main.ed_lags			optionarg main.ed_lags		}		option main.ck_force	endoptionsEND