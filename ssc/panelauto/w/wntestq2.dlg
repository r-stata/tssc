/*	wntestq2		VERSION 1.0.0  21dec2002*/VERSION 8.0INCLUDE _std_largeINCLUDE headerHELP hlp1, view("help wntestq")RESET res1DIALOG main, label("wntestq2 - Portmanteau test for white noise on single panel")	/*		*/ tabtitle("Main")BEGIN  DEFINE _x _xsetbu  DEFINE _y _top  INCLUDE _bu_tsset  TEXT     tx_var        _lft     _top     _cwd2       .,		/*		*/ label("Variable:")  VARNAME  vn_var           @      _ss    _vnwd        ., allowts	/*		*/ label("Variable")  GROUPBOX gb_opt	 _lft     _xls     _iwd     _ht2,		/*  		*/ label("Options")  CHECKBOX ck_lags    _indent      _ss    _ckwd        ., 		/*  		*/ label("")     					/*		*/ clickon("gaction main.sp_lags.enable")		/*		*/ clickoff("gaction main.sp_lags.disable")		  SPINNER  sp_lags     _cksep        @    _spwd        ., 		/*		*/ option("lags")					/*		*/ label("Number of autocorrelations")			/*		*/ min(1)						/*		*/ max(40)	  TEXT     tx_name     _spsep        @  _rbsprb        .,		/*  		*/ label("Number of autocorrelations")    		/*		*/ENDINCLUDE ifinPROGRAM commandBEGIN	put "wntestq2 "	varlist main.vn_var	INCLUDE _ifin_pr	beginoptions		optionarg main.sp_lags	endoptionsEND