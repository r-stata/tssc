program def dummies*! NJC 1.1.1 20 Oct 1998        version 5.0        local varlist "min(1)"        local if "opt"        local in "opt"        local options "Missing"        parse "`*'"        parse "`varlist'", parse(" ")        while "`1'" != "" {                if length("`1'") == 8 { local stub = substr("`1'",1,7) }                else local stub "`1'"                qui tab `1' `if' `in', gen(`stub') `missing'                mac shift        }end