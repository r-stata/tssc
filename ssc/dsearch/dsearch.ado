*! dsearch.ado 0.1 ukohler@sowi.uni-mannheim.deprogram define dsearch	version 7.0	local 0 = lower("`0'")	foreach word of local 0 {		foreach var of varlist _all {			local label: var lab `var'			local label = lower("`label'")			foreach piece of local label {				if "`word'" == "`piece'" {					local find "`find' `var'" 				}			}		}	}if "`find'" == "" {	display as result "No matches found"}else {	d `find'}endexit